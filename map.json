{
	"title": "Resumos Universidade",
	"author": "João Capucho",
	"description": "Resumos para as unidades curriculares da Licenciatura de Engenharia Informática da universidade de aveiro",
	"content": [
		{
			"name": "Fundamentos da Programação",
			"type": "builder",
			"builder": "helpers/norg_builder.py",
			"workdir": "FP",
			"args": [
				"--template=templates/Simple.html",
				"--static-assets=res"
			],
			"children": "FP/map.json"
		},
		{
			"name": "Introdução as tecnologias web",
			"type": "builder",
			"builder": "ITW/build.py",
			"workdir": "ITW",
			"args": [
				"--template-dir",
				"templates"
			],
			"children": "ITW/map.json"
		},
		{
			"name": "Modelação e análise de sistemas",
			"type": "group",
			"workdir": "MAS",
			"children": "MAS/map.json"
		},
		{
			"name": "Cálculo II",
			"type": "group",
			"workdir": "CalcII",
			"children": "maps/CálculoII.json"
		},
		{
			"name": "Modelação de Sistemas Físicos",
			"type": "builder",
			"builder": "helpers/norg_builder.py",
			"workdir": "MSF",
			"args": [
				"--template=templates/Simple.html",
				"--static-assets=assets"
			],
			"children": "MSF/map.json"
		},
		{
			"name": "Matemática Discreta",
			"type": "group",
			"workdir": "MD",
			"children": "maps/MD.json"
		}
	]
}
